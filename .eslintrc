{
	"parser": "babel-eslint",
	"plugins": [
		"prettier"
	],
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier",
		"prettier/react"
	],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"es6": true,
		"jest": true,
		"node": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"react/prop-types": 0,
		"no-extra-boolean-cast": 0
	}
}